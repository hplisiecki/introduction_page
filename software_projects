    <script>
        $(document).ready(function() {
            $('.container_main').css('opacity', '1');

            $('a').click(function(e) {
                e.preventDefault();
                newLocation = this.href;

                $('.container_main').css('opacity', '0');

                setTimeout(function() {
                    window.location = newLocation;
                }, 250);
            });
        });
    </script>
